os: mac
app: Bear
-
go (all | notes): key(cmd-1)
go untagged: key(cmd-2)
go to do: key(cmd-3)
go today: key(cmd-4)
go archive: key(cmd-5)
go trash: key(cmd-6)
go locked: key(cmd-7)
go search: key(cmd-f)
delete note: key(cmd-backspace)
display normal: key(ctrl-3)
display compact: key(ctrl-1)
new note: key(cmd-n)
end edit: key(cmd-enter)
edit: key(enter)
toggle to do: key(cmd-shift-t)

# These commands require my personal Keyboard Maestro Macros
backup: key(ctrl-alt-cmd-shift-b)